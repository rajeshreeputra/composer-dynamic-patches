{
    "name": "rajeshreeputra/composer-dynamic-patching",
    "type": "composer-plugin",
    "description": "Provides a way to patch Composer packages dynamically.",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Rajeshreeputra",
            "email": "pravin9901@gmail.com"
        }
    ],
    "require": {
        "composer-plugin-api": "^1.0 || ^2.0",
        "cweagans/composer-patches": "^2.0"
    },
    "require-dev": {
        "composer/composer": "~1.0 || ~2.0",
        "phpunit/phpunit": "^9 || ^10"
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "class": "rajeshreeputra\\ComposerDynamicPatching\\Plugin\\Plugin",
        "composer-patches": {
          "disable-resolvers": [
              "\\cweagans\\Composer\\Resolver\\PatchesFile",
              "\\cweagans\\Composer\\Resolver\\ResolverBase",
              "\\cweagans\\Composer\\Resolver\\RootComposer"
          ]
        }
    },
    "autoload": {
      "psr-4": {
        "rajeshreeputra\\ComposerDynamicPatching\\": "src"
      }
    },
    "minimum-stability": "dev"
}
